{
  "name": "oquiz",
  "version": "1.0.0",
  "description": "Quiz application using Node, Express and Sequelize !",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "db:create": "psql -U oquiz -d oquiz -f data/create_tables.sql",
    "db:populate": "psql -U oquiz -d oquiz -f data/populate_tables.sql",
    "db:rebuild": "npm run db:create && npm run db:populate"
  },
  "author": "mMormin",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.33.0"
  }
}
